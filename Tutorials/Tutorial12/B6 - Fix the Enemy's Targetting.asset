      codeBlockText: 
    - m_Type: 1
      Title:
        m_Untranslated: Create a Field to link the Player
      Text:
        m_Untranslated: 'Add a new public field to allow us to link the <go>Spawner</go>
          to the <go>Player</go>:


          <code>public Rigidbody2D target;</code>'
