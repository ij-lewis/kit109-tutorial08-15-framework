      m_InstructionText: 
      m_TutorialButtonText: 
      codeBlockText: 
    - m_Type: 1
      Title:
        m_Untranslated: Create a New Empty Game Object
      Text:
        m_Untranslated: 'Name it "Radius".

          Make it be a child of the <go>Player</go>.

          Ensure
          the position (relative to the player) is 0,0,0.'
      m_PollItems: []
      m_PollCorrectItemIndexes: 00000000
      m_Tutorial: {fileID: 0}
      m_Image: {fileID: 0}
      m_Video: {fileID: 0}
